    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        },
        {
            "name": "Launch allinone",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/apps/allinone",
        },
        {
            "name": "Launch auth",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/apps/auth",
        },
        {
            "name": "Launch tcpclient",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/apps/tcpclient",
            "args": [
                "localhost",
                "10010"
            ]
        }
    ]
}